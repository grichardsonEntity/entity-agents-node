{
  "name": "entity-agents",
  "version": "1.0.0",
  "description": "A team of 11 specialized autonomous AI agents for software development",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "valentina": "node dist/valentina/cli.js",
    "sydney": "node dist/sydney/cli.js",
    "amber": "node dist/amber/cli.js",
    "victoria": "node dist/victoria/cli.js",
    "brettjr": "node dist/brettjr/cli.js",
    "tango": "node dist/tango/cli.js",
    "sugar": "node dist/sugar/cli.js",
    "sophie": "node dist/sophie/cli.js",
    "asheton": "node dist/asheton/cli.js",
    "denisy": "node dist/denisy/cli.js",
    "harper": "node dist/harper/cli.js",
    "quinn": "node dist/quinn/cli.js"
  },
  "keywords": [
    "ai",
    "agents",
    "claude",
    "automation",
    "development",
    "typescript"
  ],
  "author": "Entity",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/grichardsonEntity/entity-agents-node.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  }
}
